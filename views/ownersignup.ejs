<body>


    <section class="body">
        <div class="register" id="re">
            <form action="/ownersignup" method="POST">
                <div class="form-group">
                    <input type="text" name="name" placeholder="Enter your name">
                </div>
                <div class="form-group">
                    <input type="text" name="username" placeholder="Enter your username">
                </div>
                <div class="form-group">
                    <input type="email" name="emailid" placeholder="Enter email_id">
                </div>
                <div class="form-group">
                    <input type="number" name="ph" placeholder="Enter your phone number">
                </div>
                <div class="form-group">
                    <input type="password" name="pass" placeholder="Enter your password">
                </div>
                <div class="form-group">
                    <input type="text" name="address" placeholder="Enter your address">
                </div>
                <button class="btn" id="s">Sign up</button>
            </form>
            <div>
                <p class="already">already registered please login</p>

                <!-- <button type="submit" class="btn" id="tl">Login</button> -->
            </div>
        </div>

