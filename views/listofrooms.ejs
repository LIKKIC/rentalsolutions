<body id="color">
    <div class="heading">
        <h1>Search here</h1>
    </div>
    <form action="/search">
        <input type="search" name="landmark" placeholder="Search by landmark" class="bar">
        <input type="submit" value="Search" class="bar" id="bt">
    </form>
    <div class="heading">
        <h1>Room List</h1>
    </div>
    
    <table  border="1" class="table">
        <tr class="tr">
            <th class="th">Name</th>
            <th class="th">Landmark</th>
            <th class="th">Address</th>
        </tr>
    
    <% if(Room.length>0){ %>
        <% for(var i = 0;i < Room.length;i++){%>
            <tr class="tr">
                <td > <a id="roomlink" href="/pg<%= Room[i].Name %>"><%- Room[i].Name %></a></td>
                <td><%- Room[i].landmark %></td>
                <td><%- Room[i].Address %></td>
            </tr>
            <%} %>
    <% }else{ %>
            <tr>
                <td>Data not found</td>
            </tr>
     <% } %>
    </table>
